<%= form_with model: @recipe_food, url: user_recipe_recipe_foods_path  do |form| %>
  <div>
    <%= form.label :food_id, style: "display: block" %>
    <%= form.collection_select :food_id, current_user.foods, :id, :name %>
  </div>

  <div>
    <%= form.label :quantity, style: "display: block" %>
    <%= form.number_field :quantity %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>